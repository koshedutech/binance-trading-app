{
  "binance": {
    "api_key": "your_api_key_here",
    "secret_key": "your_secret_key_here",
    "base_url": "https://testnet.binance.vision",
    "testnet": true
  },
  "screener": {
    "enabled": true,
    "interval": "15m",
    "min_volume": 100000,
    "min_price_change": 2.0,
    "exclude_symbols": ["BUSDUSDT", "USDCUSDT"],
    "quote_currency": "USDT",
    "max_symbols": 50,
    "screening_interval": 60
  },
  "trading": {
    "max_open_positions": 5,
    "max_risk_per_trade": 2.0,
    "dry_run": true
  },
  "scanner": {
    "enabled": true,
    "scan_interval": 30,
    "max_symbols": 50,
    "include_watchlist": true,
    "cache_ttl": 60,
    "worker_count": 10
  },
  "notification": {
    "enabled": false,
    "telegram": {
      "enabled": false,
      "bot_token": "",
      "chat_id": ""
    },
    "discord": {
      "enabled": false,
      "webhook_url": ""
    }
  },
  "risk": {
    "max_risk_per_trade": 2.0,
    "max_daily_drawdown": 5.0,
    "max_open_positions": 5,
    "position_size_method": "percent",
    "fixed_position_size": 100.0,
    "use_trailing_stop": true,
    "trailing_stop_percent": 1.0,
    "trailing_stop_activation": 1.5
  },
  "logging": {
    "level": "INFO",
    "output": "stdout",
    "json_format": true,
    "include_file": false
  }
}
